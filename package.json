{
  "name": "investorprops",
  "version": "2.1.0",
  "description": "Real Estate Investment Analysis Platform with Authentication - Refactored with component architecture",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "dev": "concurrently \"npm run dev:vercel\" \"npm run dev:railway\"",
    "dev:vercel": "vercel dev",
    "dev:railway": "cd railway-api && npm run dev",
    "dev:frontend": "vercel dev",
    "dev:backend": "cd railway-api && npm run dev",
    "dev:watch": "node scripts/dev-watcher.js",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:watch\"",
    "build": "echo 'No build step required'",
    "build:local": "vercel build",
    "deploy": "vercel --prod",
    "deploy:preview": "vercel",
    "deploy:fast": "vercel build && vercel deploy --prebuilt --prod",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:fast": "node tests/e2e/simple-fast-test.js",
    "test:api": "start-server-and-test dev:vercel http://localhost:3000 test:api:run",
    "test:api:run": "node tests/api/test-all-endpoints.js",
    "test:analysis": "start-server-and-test dev:vercel http://localhost:3000 test:analysis:run",
    "test:analysis:run": "node tests/e2e/test-analysis-page.js",
    "test:visual": "start-server-and-test dev:vercel http://localhost:3000 test:visual:run",
    "test:visual:run": "node tests/e2e/visual-check.js",
    "test:all-fast": "npm run test:api && npm run test:fast",
    "test:e2e": "start-server-and-test dev:vercel http://localhost:3000 test:e2e:run",
    "test:e2e:run": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:visual:old": "playwright test visual-analysis-test.spec.js --project=chromium",
    "test:visual:headed": "playwright test visual-analysis-test.spec.js --project=chromium --headed",
    "analyze:screenshots": "node tests/e2e/analyze-screenshots.js",
    "test:integration": "start-server-and-test dev:vercel http://localhost:3000 test:integration:run",
    "test:integration:run": "node tests/integration/test-property-analysis.js",
    "test:all": "npm run test && npm run test:e2e",
    "test:quick": "npm run test:syntax && npm run test:lint && npm run test:critical",
    "test:comprehensive": "npm run test:syntax && npm run test:e2e && npm run test:api && npm run test:visual && npm run test:mobile",
    "test:e2e:comprehensive": "start-server-and-test dev:vercel http://localhost:3000 'playwright test tests/e2e/comprehensive-e2e.spec.js'",
    "test:e2e:comprehensive:headed": "start-server-and-test dev:vercel http://localhost:3000 'playwright test tests/e2e/comprehensive-e2e.spec.js --headed'",
    "test:e2e:comprehensive:debug": "start-server-and-test dev:vercel http://localhost:3000 'playwright test tests/e2e/comprehensive-e2e.spec.js --debug'",
    "test:e2e:verify-fixes": "start-server-and-test dev:vercel http://localhost:3000 'playwright test tests/e2e/verify-fixes-test.spec.js'",
    "test:e2e:run-all": "start-server-and-test dev:vercel http://localhost:3000 'node tests/e2e/run-comprehensive-tests.js'",
    "test:syntax": "node tests/validate-javascript-syntax.js",
    "test:lint": "echo 'Lint check - add linter if needed'",
    "test:critical": "playwright test tests/e2e/critical-workflows.spec.js --reporter=list",
    "test:mobile": "start-server-and-test dev:vercel http://localhost:3000 'playwright test --project=mobile'",
    "test:extension": "node tests/extension/test-data-flow.js",
    "test:edge-cases": "start-server-and-test dev:vercel http://localhost:3000 'playwright test tests/e2e/edge-cases.spec.js'",
    "test:performance": "lighthouse ci",
    "test:security": "echo 'Security tests - add security scanner if needed'",
    "test:report": "node tests/generate-test-report.js",
    "analyze:coverage": "npm run test:coverage && open coverage/lcov-report/index.html",
    "firebase:init": "node scripts/init-firestore.js",
    "firebase:admin": "node scripts/create-admin.js",
    "firebase:deploy": "firebase deploy",
    "firebase:deploy:rules": "firebase deploy --only firestore:rules,storage:rules",
    "firebase:deploy:indexes": "firebase deploy --only firestore:indexes",
    "start": "cd railway-api && npm install && npm start",
    "railway:install": "cd railway-api && npm install",
    "railway:start": "cd railway-api && npm start",
    "prepare": "husky"
  },
  "dependencies": {
    "apify-client": "^2.12.6",
    "axios": "^1.10.0",
    "firebase-admin": "^11.11.0",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.8.1",
    "openai": "^5.9.0",
    "puppeteer": "^24.15.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@playwright/test": "^1.53.1",
    "@types/jest": "^29.5.11",
    "babel-jest": "^29.7.0",
    "concurrently": "^9.2.0",
    "dotenv": "^16.6.1",
    "express": "^5.1.0",
    "firebase-tools": "^13.9.0",
    "http-server": "^14.1.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.1.4",
    "start-server-and-test": "^2.0.12",
    "supertest": "^6.3.3",
    "vercel": "^32.5.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "node tests/validate-javascript-syntax.js"
    ],
    "*.html": [
      "node tests/validate-javascript-syntax.js"
    ],
    "api/**/*.js": [
      "node scripts/test-full-dev-setup.js"
    ],
    "railway-api/**/*.js": [
      "node scripts/test-full-dev-setup.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/investorprops.git"
  },
  "keywords": [
    "real-estate",
    "investment",
    "roi",
    "analysis",
    "firebase",
    "ai",
    "property-analysis",
    "proptech"
  ],
  "author": "InvestorProps",
  "license": "MIT",
  "refactored": {
    "deploymentId": "deploy-1752891303962",
    "deployedAt": "2025-07-19T02:15:04.007Z",
    "architecture": "component-based",
    "designSystem": "css-custom-properties",
    "responsiveDesign": "mobile-first"
  }
}
